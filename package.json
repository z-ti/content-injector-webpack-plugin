{
  "name": "content-injector-webpack-plugin",
  "version": "0.0.1",
  "description": "A webpack plugin to dynamically inject code into the bundle.",
  "keywords": [
    "webpack",
    "plugin",
    "banner",
    "header",
    "build-time",
    "inject"
  ],
  "license": "MIT",
  "author": "flyfox (https://github.com/flyfox11)",
  "bugs": {
    "url": "https://github.com/z-ti/content-injector-webpack-plugin/issues"
  },
  "homepage": "https://github.com/z-ti/content-injector-webpack-plugin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-ti/content-injector-webpack-plugin.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "postbuild": "node postbuild.js",
    "build": "npm-run-all clean build:cjs build:esm postbuild"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "webpack": ">=5.0.0"
  },
  "devDependencies": {
    "@types/webpack": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "replace-in-file": "^6.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.7.3",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}